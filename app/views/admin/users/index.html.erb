<p id="notice"><%= notice %></p>

<h1>Listing Admin Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Password</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Creation date</th>
      <th>Last login time</th>
      <th>Hood</th>
      <th>Block</th>
      <th>Phone num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_users.each do |admin_user| %>
      <tr>
        <td><%= admin_user.username %></td>
        <td><%= admin_user.email %></td>
        <td><%= admin_user.password %></td>
        <td><%= admin_user.first_name %></td>
        <td><%= admin_user.last_name %></td>
        <td><%= admin_user.creation_date %></td>
        <td><%= admin_user.last_login_time %></td>
        <td><%= admin_user.hood_id %></td>
        <td><%= admin_user.block_id %></td>
        <td><%= admin_user.phone_num %></td>
        <td><%= link_to 'Show', admin_user %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
        <td><%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admin_user_path %>
